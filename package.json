{
  "name": "form-ctrl-vanilla",
  "version": "0.10.0",
  "private": false,
  "description": "Extensible Form control Map for multiple forms with field values, state, validation",
  "keywords": [
    "form",
    "ctrl",
    "control",
    "multiple",
    "fields",
    "state",
    "store"
  ],
  "homepage": "https://github.com/applecube/form-ctrl-vanilla",
  "bugs": {
    "url": "https://github.com/applecube/form-ctrl-vanilla/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/applecube/form-ctrl-vanilla.git"
  },
  "license": "MIT",
  "author": "Alexander Lipatov",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tshy",
    "lint": "eslint --color --quiet",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jest": "^28.11.1",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "tshy": "^3.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "tshy": {
    "selfLink": false,
    "exclude": [
      "**/*.test.ts"
    ],
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts",
      "./*": "./src/*.ts"
    }
  }
}
